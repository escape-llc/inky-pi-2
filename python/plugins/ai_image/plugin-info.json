{
	"id": "ai-image",
	"name": "OpenAI Image Plugin",
	"version": "0.0.1",
	"class": "OpenAIImage",
	"file": "ai_image.py",
	"module": "python.plugins.ai_image.ai_image",
	"description": "OpenAI Gen-AI images.",
	"features": ["internet", "api-key"],
	"disabled": false,
	"settings": {
		"schema": {
			"properties": [
				{
					"name": "apiKey",
					"type": "string",
					"required": true,
					"default": null,
					"label": "API Key",
					"description": "Your OpenAI API key."
				}
			]
		},
		"default": {
			"apiKey": null
		}
	},
	"instanceSettings": {
		"schema": {
			"properties": [
				{
					"name": "imageModel",
					"type": "string",
					"required": true,
					"default": "dall-e-3",
					"label": "Image Model",
					"description": "OpenAI image model.",
					"enum": ["dall-e-3", "dall-e-2", "gpt-image-1"]
				},
				{
					"name": "imageQuality",
					"type": "string",
					"required": true,
					"default": "medium",
					"label": "Image Quality",
					"description": "OpenAI image quality.",
					"enum":["low","medium","high"]
				},
				{
					"name": "prompt",
					"type": "string",
					"required": true,
					"default": null,
					"label": "Prompt",
					"description": "Your Gen-AI prompt for the image."
				},
				{
					"name": "randomizePrompt",
					"type": "boolean",
					"required": false,
					"default": true,
					"label": "Randomize Prompt",
					"description": "Randomize the prompt."
				},
				{
					"name": "slideshowMinutes",
					"type": "number",
					"required": true,
					"default": 15,
					"min": 1,
					"label": "Slideshow Minutes",
					"description": "Minutes per image."
				}
			]
		},
		"default": {
			"imageModel": "dall-e-3",
			"imageQuality": "standard",
			"prompt": null,
			"randomizePrompt": false,
			"slideshowMinutes": 15
		}
	}
}