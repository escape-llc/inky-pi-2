{
	"id": "newspaper",
	"name": "Newspaper Picture-of-the-Day",
	"version": "0.0.1",
	"class": "Newspaper",
	"file": "newspaper.py",
	"module": "python.plugins.newspaper.newspaper",
	"description": "A Newspaper PotD downloader.",
	"disabled": false,
	"features": ["internet"],
	"settings": {
		"schema": {
			"properties": [
			]
		},
		"default": null
	},
	"instanceSettings": {
		"schema": {
			"lookups": {
				"newspaperSlug": {
					"url": "/api/lookups/newspaperSlug"
				}
			},
			"properties": [
				{
					"name": "newspaperSlug",
					"type": "string",
					"required": true,
					"default": "ny_nyt",
					"label": "Newspaper",
					"lookup": "newspaperSlug",
					"description": "Select the newspaper."
				},
				{
					"name": "slideshowMinutes",
					"type": "number",
					"required": true,
					"default": 15,
					"min": 1,
					"label": "Slideshow Minutes",
					"description": "Minutes per image."
				}
			]
		},
		"default": {
			"newspaperSlug": "ak_adn",
			"slideshow": false,
			"slideshowMinutes": 15
		}
	}
}